{
  "name": "wdd331-group5",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build-css": "node-sass -r -o src/ src/scss/",
    "watch-html": "parcel src/index.html",
    "watch-css": "node-sass --watch -r -o src/ src/scss/",
    "start": "concurrently \"npm:watch-css\" \"npm:watch-html\"",
    "predeploy": "rm -rf dist && npm run build-css && parcel build src/index.html --public-url https://ebrentnelson.github.io/wdd331-group5/",
    "deploy": "gh-pages -d dist"
  },
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.0.0",
    "gh-pages": "^3.2.3",
    "node-sass": "^7.0.1",
    "parcel": "^2.3.0",
    "prettier": "^2.5.1"
  }
}
